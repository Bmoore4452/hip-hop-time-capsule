{
  "expo": {
    "name": "Hip-Hop Time Capsule",
    "slug": "hip-hop-time-capsule",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/book-cover-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/book-cover-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4c2c92"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.teamscipiomusic.hip-hop-time-capsule"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/book-cover-adaptive.png",
        "backgroundColor": "#4c2c92"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.teamscipiomusic.hiphoptimecapsule"
    },
    "web": {
      "favicon": "./assets/book-cover-favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
}
